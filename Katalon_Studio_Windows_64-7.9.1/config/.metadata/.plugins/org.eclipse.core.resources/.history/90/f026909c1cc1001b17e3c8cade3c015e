CustomKeywords.'loginWithClientUesr.Login'('hoangmen200799@gmail.com', 'hoangmen99')

WebUI.click(findTestObject('Page_TrangChu/icon_User'))

WebUI.click(findTestObject('Page_TrangChu/span_ThongTinCaNhan'))

WebUI.waitForElementPresent(findTestObject('Page_TaiKhoan/input_Ho'), 5)

WebUI.setText(findTestObject('Page_TaiKhoan/input_Ho'), 'Hoang')

WebUI.setText(findTestObject('Page_TaiKhoan/input_Ten'), 'Men')

WebUI.setText(findTestObject('Page_TaiKhoan/input_SoDT'), '0372671527')

WebUI.selectOptionByLabel(findTestObject('Page_TaiKhoan/select_GioiTinh'), 'Nữ', true)

WebUI.selectOptionByValue(findTestObject('Page_TaiKhoan/select_NgaySinh'), '20', true)

WebUI.selectOptionByValue(findTestObject('Page_TaiKhoan/select_ThangSinh'), '07', true)

WebUI.selectOptionByValue(findTestObject('Page_TaiKhoan/select_NamSinh'), '1999', true)

WebUI.setText(findTestObject('Page_TaiKhoan/input_Masothue'), '123')

WebUI.setText(findTestObject('Page_TaiKhoan/input_TenDonVi'), 'GHTK')

WebUI.setText(findTestObject('Page_TaiKhoan/input_DiaChi'), '12/38 Khuc Thu Du,  Dich Vong,Cau Giay,')

WebUI.click(findTestObject('Page_TaiKhoan/span_CapNhat'))

WebUI.verifyTextPresent('Cập nhật thành công', false)

WebUI.refresh()

WebUI.verifyElementAttributeValue(findTestObject('Page_TaiKhoan/input_Ho'), 'value', 'Hoang', 3)

WebUI.verifyElementAttributeValue(findTestObject('Page_TaiKhoan/input_Ten'), 'value', 'Men', 3)

WebUI.verifyElementAttributeValue(findTestObject('Page_TaiKhoan/input_DiaChi'), 'value', '12/38 Khuc Thu Du,  Dich Vong,Cau Giay,', 
    3)

WebUI.verifyElementAttributeValue(findTestObject('Page_TaiKhoan/input_SoDT'), 'value', '0372671527', 3)

WebUI.verifyElementAttributeValue(findTestObject('Page_TaiKhoan/input_MaSoThue'), 'value', '123', 3)

WebUI.verifyElementAttributeValue(findTestObject('Page_TaiKhoan/input_TenDonVi'), 'value', 'GHTK', 3)

